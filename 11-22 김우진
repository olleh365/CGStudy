        glVertex2f(0.112 * x - 0.512 + (0.007 * i), y * 0.196 + 0.106 + (0.01225 * -i));
        glVertex2f(0.112 * x - 0.512 + (0.007 * i), y * 0.196 + -0.09 + (0.01225 * 0));

    }
