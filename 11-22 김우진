    glVertex2f(0.112 * x - 0.4, y * 0.196 + -0.07775);
    glVertex2f(0.112 * x - 0.512, y * 0.196 + -0.07775);
    glVertex2f(0.112 * x - 0.512, y * 0.196 + -0.09);

    glColor3f(0, 0, 0);
